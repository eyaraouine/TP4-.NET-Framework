@model List<Student>

<h1>Students Enrolled in @ViewData["course"]</h1>
<br>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">First Name</th>
            <th scope="col">Last Name</th>
            <th scope="col">Phone Number</th>
            <th scope="col">University</th>
            <th scope="col">Timestamp</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var p in Model)
        {
        <tr>
            <th scope="row">
                @p.Id
            </th>
            <td>
                @p.First_Name
            </td>
            <td>
                @p.Last_Name
            </td>
            <td>
                @p.Phone_Number
            </td>
            <td>
                @p.University
            </td>
            <td>
                @p.Timestamp
            </td>
        </tr>
        }
    </tbody>
</table>

